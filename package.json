	},
	"homepage": "https://github.com/brhamidi/web-tetris#readme",
	"devDependencies": {
		"@babel/cli": "^7.2.3",
		"@babel/core": "^7.2.2",
		"@babel/node": "^7.2.2",
		"@babel/preset-env": "^7.3.1",
		"@babel/preset-react": "^7.0.0",
		"babel-jest": "^24.5.0",
		"babel-loader": "^8.0.5",
		"enzyme": "^3.9.0",
		"enzyme-adapter-react-16": "^1.11.2",
		"jest": "^24.5.0",
