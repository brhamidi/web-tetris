{
	"name": "web-tetris",
	"version": "1.0.0",
	"description": "Multiplayer networked web Tetris game",
	"scripts": {
		"srv-dev": "nodemon --exec babel-node -w src/server src/server/main.js",
		"srv-prod": "babel --out-dir dist/server src/server",
		"client-dev": "webpack-dev-server --host 0.0.0.0",
		"client-prod": "webpack --progress",
		"coverage": "jest --coverage",
		"test": "jest --maxWorkers=2"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/brhamidi/web-tetris.git"
	},
